# GAMING FRONTEND - ENVIRONMENT CONFIGURATION
# Copy this file to .env and update for your environment
# DO NOT commit .env to version control!

# BACKEND API (Required)
VITE_API_BASE_URL=http://localhost:8000/api #Replace localhost with PC IP 

# PALM BRIDGE (for web kiosk palm scanning)
VITE_PALM_BRIDGE_URL=http://192.168.X.X:8088 #check the correct Palm Bridge IP

# GEOAPIFY (for maps)
VITE_GEOAPIFY_API_KEY=your_api_key_here

# PUSH NOTIFICATIONS (VAPID key)
VITE_VAPID_PUBLIC_KEY=your_vapid_key_here
